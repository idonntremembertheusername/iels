<view class="page" data-weui-theme="{{theme}}">
	
		<view class="weui-form__text-area">
			<image src="../../images/iels.png"/>
		</view>
		<view class="weui-form__control-area">
			<view class="weui-cells__group weui-cells__group_form">
				<view class="weui-cells__title">请先填写注册信息</view>
				<view class="weui-cells weui-cells_form">
					<view class="weui-cell weui-cell_active">
						<view class="weui-cell__hd"><label class="weui-label">帐号</label></view>
						<view class="weui-cell__bd">
							<input class="weui-input" placeholder="填写帐号" placeholder-class="weui-input__placeholder" />
						</view>
					</view>
					<view class="weui-cell weui-cell_active">
						<view class="weui-cell__hd">							
                            <label class="weui-label">密码</label>
                        </view>
						<view class="weui-cell__bd">
							<input class="weui-input" placeholder="填写密码" placeholder-class="weui-input__placeholder" />
						</view>
					</view>
					<view class="weui-cell weui-cell_active">
						<view class="weui-cell__hd">							
                            <label class="weui-label">确认密码</label>
                        </view>
						<view class="weui-cell__bd">
							<input class="weui-input" placeholder="填写确认密码" type="number" placeholder-class="weui-input__placeholder" />
						</view>
					</view>
				</view>
			</view>
		</view>
		<view class="weui-form__tips-area">
			
		</view>
		<!-- <view class="weui-form__opr-area"> -->
    <view class="button-sp-area"> 
			<!-- <a class="weui-btn weui-btn_primary">确定</a> -->
      <button type="default" plain="true">注册</button>
		</view>
		
		<view class="weui-form__extra-area">
			<view class="weui-footer">
				
				<view class="weui-footer__text">Copyright © 2019-2020 iels.com</view>
			</view>
		</view>
	
</view>

